#include <stdio.h>

int main() {
    int arr[5], max;
    printf("5개의 숫자를 입력하세요: ");
    for (int i = 0; i < 5; i++) {
        scanf("%d", &arr[i]);
    }
    max = arr[0];
    for (int i = 1; i < 5; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
    }
    printf("최대값: %d\n", max);
    return 0;
}
